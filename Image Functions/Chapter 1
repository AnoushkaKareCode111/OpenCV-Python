import cv2
import numpy as np

img=cv2.imread("C:\\Users\\Janendra Kaushik\\Pictures\\Look Out Mountain Look Out Sea linocut print.jpg")
kernels=np.ones((5,5),np.uint8)
  
#BLACK & WHITE CONVERSION
bwimg=cv2.cvtColor(img,cv2.COLOR_BGR2GRAY)

#BLURRING AN IMAGE
blur=cv2.GaussianBlur(bwimg,(7,7),0)

#EDGE DETECTION
edge=cv2.Canny(blur,100,120)

#DILATION OF AN IMAGE
dilate=cv2.dilate(edge,kernels,iterations=1)

#EROSION OR THINNING 
erode=cv2.erode(dilate,kernels,iterations=2)
cv2.imshow("bw",erode)
cv2.waitKey(0)
